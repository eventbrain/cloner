{
    "name": "eventbrain/cloner",
    "description": "A trait for Laravel Eloquent models that lets you clone of a model and it's relationships, including files.",
    "license": "MIT",
    "authors": [
        {
            "name": "Marius Meybert",
            "email": "info@eventbrain.de"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/support": "^5.5|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0|~8.0|^9.5.10|^10.5",
        "illuminate/database": "^5.5|^6.0|^10.0|^11.0|^12.0",
        "league/flysystem-vfs": "^1.0",
        "mockery/mockery": "^1.2.3",
        "satooshi/php-coveralls": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Eventbrain\\Cloner\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eventbrain\\Cloner\\Stubs\\": "stubs/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Eventbrain\\Cloner\\ServiceProvider"
            ]
        }
    }
}
